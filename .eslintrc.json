{
  "env": {
    "es2021": true,
    "node": true,
    "jest": true
  },
  "extends": ["plugin:@typescript-eslint/recommended", "plugin:prettier/recommended"],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "plugins": ["@typescript-eslint"],
  "rules": { "@typescript-eslint/no-unused-vars": "off" }
}
