{
  "name": "keycloak-jest",
  "version": "0.1.4",
  "description": "keycloak-jest",
  "main": "src/app.ts",
  "engines": {
    "node": ">=14.16.1"
  },
  "scripts": {
    "start": "npm run build:pro && node dist/server.js",
    "lint": "npx eslint .",
    "lint:fix": "npx eslint --fix",
    "lint:lockfile": "lockfile-lint --path package-lock.json --type npm --validate-https --allowed-hotst npm",
    "deps": "npm run deps:audit && npm run deps:sec-scan",
    "deps:audit": "npm audit && npm outdated -l",
    "deps:sec-scan": "npm run lint:lockfile",
    "build:dev_server": "webpack --config ./webpack/webpack.dev.js",
    "build:dev": "npm run dev_infra:up && webpack --config ./webpack/webpack.dev.js",
    "build:pro": "webpack --config ./webpack/webpack.prod.js",
    "dev_infra:up": "docker-compose -f docker/docker-compose.yml --env-file docker/dev/.docker.env up -d",
    "dev_infra:down": "docker-compose -f docker/docker-compose.yml --env-file docker/dev/.docker.env down -v",
    "test:only": "jest --runInBand --detectOpenHandles",
    "test_infra:up": "docker-compose -f docker/docker-compose.yml --env-file docker/test/.docker.env up -d",
    "test_infra:down": "docker-compose -f docker/docker-compose.yml --env-file docker/test/.docker.env down -v"
  },
  "lint-staged": {
    "*.ts": "npm run lint:fix"
  },
  "devDependencies": {
    "@types/bluebird": "^3.5.33",
    "@types/body-parser": "^1.19.0",
    "@types/express": "^4.17.9",
    "@types/express-session": "^1.17.3",
    "@types/jest": "^26.0.15",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/node": "^14.14.9",
    "@types/supertest": "^2.0.10",
    "@typescript-eslint/eslint-plugin": "^4.8.1",
    "@typescript-eslint/parser": "^4.8.1",
    "clean-webpack-plugin": "^3.0.0",
    "dotenv": "^8.2.0",
    "eslint": "^7.13.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-promise": "^5.1.0",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.4",
    "lockfile-lint": "^4.3.7",
    "nodemon-webpack-plugin": "^4.3.2",
    "prettier": "2.2.1",
    "supertest": "^6.0.1",
    "ts-jest": "^26.4.4",
    "ts-loader": "^8.0.11",
    "ts-node": "^9.0.0",
    "tsconfig-paths-webpack-plugin": "^3.3.0",
    "typescript": "^4.1.2",
    "webpack": "^5.6.0",
    "webpack-cli": "^4.2.0",
    "webpack-merge": "^5.4.0",
    "webpack-node-externals": "^2.5.2"
  },
  "dependencies": {
    "@types/axios": "^0.14.0",
    "bluebird": "^3.7.2",
    "body-parser": "^1.19.0",
    "express": "^4.17.1",
    "helmet": "^4.4.1",
    "i": "^0.3.6",
    "joi": "^17.3.0",
    "jsonwebtoken": "^8.5.1",
    "keycloak-connect": "^12.0.4",
    "log4js": "^6.3.0",
    "npm": "^7.13.0",
    "uuidv4": "^6.2.8"
  }
}
